{
  "plugins": [
    "react",
    "prettier"
  ],
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "global-require": "off",
    "react/prefer-stateless-function": "off",
    "linebreak-style": "off",
    "radix": "off",
    "react/jsx-one-expression-per-line": 0,
    "import/prefer-default-export": 0
  },
  "parser": "babel-eslint"
}
